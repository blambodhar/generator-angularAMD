{
  "configVars": {
    "app": "app",
    "appjs": "scripts",
    "appcss": "styles",
    "build": "build",
    "dist": "dist",
    "hostname": "0.0.0.0",
    "livereloadPort": 35729,
    "port": 8080
  },
  "extdeps": {
    "vers": {
      "angular": "1.2.16"
    },
    "bower": {
      "angular": {
        "version": "<%= extdeps.vers.angular %>",
        "jsfiles": {
          "angular/angular.js": {
            "min": true
          }
        }
      },
      "angular-animate": {
        "version": "<%= extdeps.vers.angular %>",
        "jsfiles": {
          "angular-animate/angular-animate.js": {
            "min": true,
            "ngName": "ngAnimate",
            "shim": ["angular"]
          }
        }
      },
      "angular-cookies": {
        "version": "<%= extdeps.vers.angular %>",
        "jsfiles": {
          "angular-cookies/angular-cookies.js": {
            "min": true,
            "ngName": "ngCookies",
            "shim": ["angular"]
          }
        }
      },
      "angular-resource": {
        "version": "<%= extdeps.vers.angular %>",
        "jsfiles": {
          "angular-resource/angular-resource.js": {
            "min": true,
            "ngName": "ngResource",
            "shim": ["angular"]
          }
        }
      },
      "angular-route": {
        "version": "<%= extdeps.vers.angular %>",
        "jsfiles": {
          "angular-route/angular-route.js": {
            "min": true,
            "ngName": "ngRoute",
            "shim": ["angular"]
          }
        }
      },
      "angular-sanitize": {
        "version": "<%= extdeps.vers.angular %>",
        "jsfiles": {
          "angular-sanitize/angular-sanitize.js": {
            "min": true,
            "ngName": "ngSanitize",
            "shim": ["angular"]
          }
        }
      },
      "angular-touch": {
        "version": "<%= extdeps.vers.angular %>",
        "jsfiles": {
          "angular-touch/angular-touch.js": {
            "min": true,
            "ngName": "ngTouch",
            "shim": ["angular"]
          }
        }
      },
      "angularAMD": {
        "version": "https://github.com/marcoslin/angularAMD.git#v0.2.0-rc.1",
        "jsfiles": {
          "angularAMD/dist/angularAMD.js": {
            "min": true,
            "shim": ["angular"]
          },
          "angularAMD/dist/ngload.js": {
            "min": true,
            "shim": ["angularAMD"]
          }
        }
      },
      "jquery": {
        "version": "2.1.1",
        "jsfiles": {
          "jquery/dist/jquery.js": {
            "min": true
          }
        }
      },
      "requirejs": {
        "version": { "requirejs": "2.1.14" },
        "jsfiles": {
          "requirejs/require.js": {
            "min": false
          }
        }
      },
      "bootstrap": {
        "version": "~3.2.0",
        "jsfiles": {
          "bootstrap/dist/js/bootstrap.js": {
            "min": true
          }
        },
        "cssfiles": [
          {
            "bootstrap/dist/css/bootstrap.css": {
              "min": false
            }
          },
          {
            "bootstrap/dist/css/bootstrap-theme.css": {
              "min": false
            }
          }
        ],
        "cssfonts": [
          "bootstrap/dist/fonts/glyphicons-halflings-regular.eot",
          "bootstrap/dist/fonts/glyphicons-halflings-regular.svg",
          "bootstrap/dist/fonts/glyphicons-halflings-regular.ttf",
          "bootstrap/dist/fonts/glyphicons-halflings-regular.woff"
        ]
      },
      "es5-shim": {
        "version": "~3.4.0",
        "jsfiles": {
          "es5-shim/es5-shim.js": {
            "min": true,
            "htmlOnly": true
          }
        }
      },
      "json3": {
        "version": "~3.3.2",
        "jsfiles": {
          "json3/lib/json3.js": {
            "min": true,
            "htmlOnly": true
          }
        }
      }
    },
    "npm": {
      "grunt": "^0.4.5",
      "grunt-autoprefixer": "^0.7.3",
      "grunt-bower-task": "~0.3.2",
      "grunt-concurrent": "^0.5.0",
      "grunt-contrib-concat": "^0.4.0",
      "grunt-contrib-connect": "^0.7.1",
      "grunt-contrib-copy": "^0.5.0",
      "grunt-contrib-cssmin": "^0.9.0",
      "grunt-contrib-htmlmin": "^0.3.0",
      "grunt-contrib-jshint": "^0.10.0",
      "grunt-contrib-uglify": "^0.4.0",
      "grunt-contrib-watch": "^0.5.3",
      "grunt-filerev": "^0.2.1",
      "grunt-google-cdn": "^0.4.0",
      "grunt-newer": "^0.7.0",
      "grunt-ngmin": "^0.0.3",
      "grunt-svgmin": "^0.4.0",
      "grunt-usemin": "^2.1.1",
      "jshint-stylish": "^0.2.0",
      "load-grunt-tasks": "^0.4.0",
      "time-grunt": "^0.3.1"
    }
  },
  "askFor": {
    "modules": {
      "type": "checkbox",
      "name": "modules",
      "message": "Which modules would you like to include?",
      "choices": [
        {
          "value": "jquery",
          "name": "jquery"
        },
        {
          "value": "angular-animate",
          "name": "angular-animate"
        },
        {
          "value": "angular-cookies",
          "name": "angular-cookies"
        },
        {
          "value": "angular-resource",
          "name": "angular-resource"
        },
        {
          "value": "angular-sanitize",
          "name": "angular-sanitize"
        },
        {
          "value": "angular-touch",
          "name": "angular-touch"
        }
      ]
    }

  }
}
